<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <!-- EmguCV paketleri NuGet Package Manager veya dotnet add package komutu ile yüklenmiştir -->
    <!-- Örnek komut: dotnet add package Emgu.CV -->
    <!-- Örnek komut: dotnet add package Emgu.CV.Bitmap -->
    <!-- Örnek komut: dotnet add package Emgu.CV.runtime.windows -->
    <PackageReference Include="Emgu.CV" Version="4.9.0.5494" />
    <PackageReference Include="Emgu.CV.Bitmap" Version="4.9.0.5494" />
    <PackageReference Include="Emgu.CV.runtime.windows" Version="4.9.0.5494" />
    <PackageReference Include="Npgsql" Version="10.0.1" />
  </ItemGroup>

</Project>